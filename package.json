  "description": "dashactyl is a client dashboard for the pterodactyl game panel",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "clean:win": "rmdir /q /s build && mkdir build",
    "clean:lnx": "rm -rf build && mkdir build",
    "dev": "npm i -D @types/ejs @types/express @types/ecpress-session @types/mongoose @types/node-fetch ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Votion-Development/Dashactyl-v2.git"
  },
  "keywords": [
    "dashactyl",
    "pterodactyl",
    "dashboard",
    "client",
    "api"
  ],
