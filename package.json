{
  "name": "dashactyl",
  "version": "3.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:console": "tsc -p tsconfig.cmd.json",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "c:settings:init": "node ./build/settings-init",
    "c:user:create": "node ./build/user-create",
    "c:user:delete": "node ./build/user-delete",
    "c:user:list": "node ./build/user-list",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "migrate": "prisma migrate dev",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.16",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@remix-run/server-runtime": "^1.7.6",
    "axios": "0.27.2",
    "bcryptjs": "^2.4.3",
    "commander": "^9.4.1",
    "isbot": "^3.6.3",
    "react": "^18.2.0",
    "react-arc-progress": "^1.0.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "binode": "^1.0.5",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}