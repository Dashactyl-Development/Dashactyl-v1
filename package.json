  "contributors": [
    "Devonte <https://github.com/devnote-dev>",
    "Cblaze <https://github.com/blazecoding2009>",
    "Jamie <https://github.com/JamieGrimwood>"
  ],
  "license": "MIT",
  "dependencies": {
    "connect-mongo": "^4.6.0",
    "ejs": "^3.1.7",
    "express": "^4.18.1",
    "express-session": "^1.17.2",
    "mongoose": "^6.3.2",
    "node-fetch": "^2.6.7",
    "typescript": "^4.6.4",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
