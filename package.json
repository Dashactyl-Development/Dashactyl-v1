{
  "name": "dashactyl",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently -r \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start"
  },
  "workspaces":[
    "./client",
    "./server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Votion-Development/Dashactyl.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Votion-Development/Dashactyl/issues"
  },
  "homepage": "https://github.com/Votion-Development/Dashactyl#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
