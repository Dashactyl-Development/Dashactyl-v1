{
  "name": "dashactyl-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Votion-Development/Dashactyl"
  },
  "scripts": {
    "dev": "concurrently -r \"vite\" \"tailwindcss -o ./dist/tailwind.css --watch\"",
    "build": "tsc && vite build && tailwindcss -o ./dist/tailwind.css",
    "preview": "vite preview",
    "format": "prettier --write ./src/**/*.{ts,tsx}"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "concurrently": "^7.6.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  }
}
